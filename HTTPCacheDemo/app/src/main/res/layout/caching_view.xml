<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="70dp"
    android:layout_centerVertical="true">

    <TextView
        android:id="@+id/hascached_caching"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="16dp"
        android:layout_marginLeft="16dp"
        android:text="aaaaa"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@+id/hascached_caching"
        android:layout_marginLeft="16dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tittle_caching"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            android:maxLength="10"
            android:singleLine="true"
            android:layout_marginTop="8dp"
            android:text="aaaa"/>
        <TextView
            android:id="@+id/url_caching"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12dp"
            android:singleLine="true"
            android:layout_marginTop="5dp"
            android:maxLength="33"
            android:layout_below="@+id/tittle_caching"
            android:text="bbbb"/>
    </LinearLayout>

</RelativeLayout>